@model List<licenta.Models.myIncidentRequestViewModel>


@if (Model.Count > 0)
{

    <table class="table">
        <tr>
            <td>Type</td>
            <td>Title</td>
            <td>DepartmentAssigned</td>
            <td>Status</td>
            <td>Priority</td>

        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.IncidentRequest)
                </td>
                <td>
                    <a href="~/Home/History?id=@item.Id">@Html.DisplayFor(modelItem => item.Title)</a>
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DepartmentAssigned)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Status)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Priority)
                </td>

            </tr>


        }
    </table>}
else
{
    <p style="color:red;">No elements available</p>
}


@Scripts.Render("~/bundles/jquery")
<script>
    $(document).ready(function () {
        setInterval(function () {
            cache_clear()
        }, 30000);
    });

    function cache_clear() {
        console.log("test");
        window.location.reload();
    }
</script>
